all_data_join %>%
ggplot(aes(x = cond, y = R4, colour = cond)) + ggtitle("RI for Critical Region: Question") +
labs(y = "Reading time in ms.", x = "Prediction") +
geom_violin() +
geom_jitter(alpha = .2, width = .1) +
stat_summary(fun.data = "mean_cl_boot", colour = "black") +
guides(colour = FALSE)
